<?xml version="1.0"?>
<!DOCTYPE mbeans-descriptors PUBLIC
        "-//Apache Software Foundation//DTD Model MBeans Configuration File"
        "http://jakarta.apache.org/commons/dtds/mbeans-descriptors.dtd">
<mbeans-descriptors>
    <mbean name="BlendedRealm" className="org.apache.catalina.mbeans.ClassNameMBean"
           description="A combined realm, with caching, which can retrieve additional roles separately"
           domain="Catalina"
           group="Realm"
           type="org.silverdream.apache.catalina.realm.BlendedRealm">

        <attribute name="cacheTime"
                   description="Time to cache results for"
                   type="int" />

        <attribute name="cacheTimeUnit"
                     description="Unit of time, h, m or d (hours, minutes, or days)"
                     type="java.lang.String" />
    </mbean>
</mbeans-descriptors>
